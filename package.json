{
  "name": "@reveldigital/reveldigital-mcp-server",
  "description": "MCP Server generated from OpenAPI spec for revel-digital-rest-api",
  "private": false,
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "@reveldigital/reveldigital-mcp-server": "./build/index.js"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "revel-digital",
    "mcp"
  ],
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc && chmod 755 build/index.js",
    "typecheck": "tsc --noEmit",
    "prestart": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "axios": "^1.9.0",
    "dotenv": "^16.4.5",
    "zod": "^3.24.3",
    "json-schema-to-zod": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "version": "1.0.12",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RevelDigital/reveldigital-mcp-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RevelDigital/reveldigital-mcp-server/issues"
  },
  "homepage": "https://github.com/RevelDigital/reveldigital-mcp-server#readme"
}
